
<% if @feed_items.any? %>
  <ol class = "posts">
    <% i = 0 %>
    <% @feed_items.each do |feed_item| %>
    <%= render 'shared/feed_item', feed_item: feed_item, i: i, hash: @hash %>
    <% i += 1 %>
    <% end %>
  </ol>
    <% if !no_will_paginate %>
      <%= will_paginate @feed_items  %>
    <% end %>
<% end %>

