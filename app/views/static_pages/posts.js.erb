$('.pre-posts').append('<%= j render( partial: "shared/feed", loccals: {:@feed_items => @feed_items}) %>');

<% if @feed_items.next_page %>
 if (!$('.pagination').length > 1){
  $('.pagination').replaceWith('<%= j will_paginate(@feed_items) %>');
  }
<% else %>
  $('.pagination').remove();
<% end %>
<% sleep 1 %>
