<% unless @poll_posts.empty? %>

  $('div.poll').html("<a href='' id='poll' style='background: yellow; color: blue; text-align: center'>New Posts added</a>");

$('.pre-posts')
       .prepend('<%= j render( partial: "shared/feed", locals: {:@feed_items => @poll_posts, no_will_paginate: true}) %>');
<% end %>

